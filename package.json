{
  "name": "storefrontbackend",
  "version": "1.0.0",
  "description": "Store Front Backend",
  "main": "server.js",
  "license": "ISC",
  "author": "Amr Yassen",
  "scripts": {
    "mdevup": "db-migrate --env dev up && db-migrate up",
    "mdevdown": "db-migrate --env dev reset && db-migrate down",
    "mtestup": "db-migrate --env test up && db-migrate up",
    "mtestdown": "db-migrate --env test reset && db-migrate down",
    "test": "SET ENV=test&npm run mtestup&npm run build& jasmine&npm run mtestdown",
    "dev": "nodemon ./src/server.ts",
    "clean": "rimraf build/",
    "build": "npm run clean && npx tsc",
    "start": "npm run build & node build/server.js",
    "lint": "eslint src/**/*.ts",
    "fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.12",
    "@types/pg": "^8.6.5",
    "@types/pg-format": "^1.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pg-format": "^1.0.4"
  }
}
